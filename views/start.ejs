    <div ng-app>
        <div class="row" ng-controller="StartCtrl">
            <h2>Existing tournaments</h2>
            <table class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th>Date</th>
                        <th>Name</th>
                    </tr>
                </thead>
                <tbody>
                    <tr ng-repeat="tournament in tournaments">
                        <td>{{tournament.no}}</td>
                        <td>{{tournament.name}}</td>
                    </tr>
                </tbody>
            </table>
        </div>
    
        <div class="row">    
            <div class="well">
                <a href="#" id="createTournament" class="btn btn-primary">Create new tournament</a>
            </div>
        </div>
    </div>

    <script src="/js/lib/underscore.js"></script>
    <script src="/js/lib/angular.min.js"></script>
    <script src="/js/lib/alertify.min.js"></script>
    <script src="/js/brisco/briscoGame.js"></script>
    <script src="/js/brisco/briscoScore.js"></script>
    <script src="/js/brisco/briscoHtml.js"></script>
    <script src="/js/db.js"></script>
    <script src="/js/start.js"></script>